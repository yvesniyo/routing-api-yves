            <div class="main-header_update">
                <STRONG style="color: #e67e22"><i class="fa fa-warning"></i> System Alerts</STRONG>
                <input value="<%= data.email ;%>" type="hidden" id="" />
                <div class="alerts">
                    <% if (!data.confirmed) { %>
                        <div class="alert some">
                            <p>Please you have to confirm your email,because you won't be accessing all the actities of this system</p>
                        </div>
                        <div class="alert some" style="background-color: #cce5ff;">
                                <p style="color: #6f8bff">Or Resend Confirmation Link :
                                    <label style="color: green;"><a href="#resend" id="resendLink">Resend</a></label>
                                </p>
                        </div>
                    <% }  %>
                    
                    

                </div>
            </div>
            <div class="main-overview">
                <% if (!bussiness) { %>
                
                <div class="overviewcard">
                    <div class="overviewcard__icon fa fa-ticket"></div>
                    <div class="overviewcard__info">
                        <h6>Requests</h6>
                        <p>
                            <% 
							let amount=data.amount;
							let req=amount/30;
							%>
                                <%= Math.floor(parseInt(req),2) %>
                        </p>
                    </div>
                </div>
                <div class="overviewcard">
                    <div class="overviewcard__icon fa fa-money"></div>
                    <div class="overviewcard__info">
                        <h6>Amount Left</h6>
                        <p><%= Math.floor(parseInt(data.amount)); %> Rwf</p>
                    </div>
                </div>
                <div class="overviewcard">
                    <div class="overviewcard__icon fa fa-home"></div>
                    <div class="overviewcard__info">
                        <h6>Last Payment</h6>
                        <p>
                            <%= Math.floor(data.amount-data.lastPayment) %> Rwf
                        </p>
                    </div>
                </div>
                <% }else{ %>
                   
                    <div class="overviewcard">
                        <div class="overviewcard__icon far fa-clock"></div>
                        <div class="overviewcard__info">
                            <h6>Remaining Days</h6>
                            <p>
                                <%
                                function parseDate(str) {
                                    var mdy = str.split('/');
                                    return new Date(mdy[2], mdy[0]-1, mdy[1]);
                                }
                                function datediff(f, s) { return Math.round((s-f)/(1000*60*60*24)); } %> 
                                <%= datediff( new Date().getTime(), data.numberOfMonths) %> Days
                            </p>
                        </div>
                    </div>
                <% } %>


            </div>
            <div class="main-cards">

                <div id="wrapper" class="card">
                    <div style="height: 100px;">
                        <div class="chart1" style="height: 20px;width: 20px;background-color: #269ffb">

                        </div>
                        <p style="color:#269ffb;display: inline;margin-top:-10px;cursor: pointer;"> Money</p> <br />
                        <div class="chart1" style="height: 20px;width: 20px;background-color: #6d838d">

                        </div>
                        <p style="color:#6d838d;display: inline;margin-top:-10px;cursor: pointer;"> Requests</p> <br />
                        <div class="chart1" style="height: 20px;width: 20px;background-color: #00e396">

                        </div>
                        <p style="color:#00e396;display: inline;margin-top: -10px;cursor: pointer;"> Payments</p> <br />
                    </div>
                    <div id="chart-line">
                        <apexchart type=line height=250 :options="chartOptionsLine1" :series="series1" />
                    </div>

                    <div id="chart-line2">
                        <apexchart type=line height=160 :options="chartOptionsLine2" :series="series2" />
                    </div>

                    <div id="chart-area">
                        <apexchart type=area height=160 :options="chartOptionsArea" :series="series3" />
                    </div>
                </div>
                <div class="card" style="display: none;">Content Up Here
                </div>
                <div class="card" style="display: none;">Content Up Here
                </div>
            </div>


