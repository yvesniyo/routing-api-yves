         <span class="hljs-attr">"entry"</span> : [ <span class="hljs-string">"true"</span>, <span class="hljs-string">"true"</span>, <span class="hljs-string">"true"</span>, <span class="hljs-string">"false"</span> ],
         <span class="hljs-attr">"location"</span> : [ <span class="hljs-number">13.39677</span>, <span class="hljs-number">52.54366</span> ]
      },
      {  <span class="hljs-attr">"out"</span> : <span class="hljs-number">1</span>,
